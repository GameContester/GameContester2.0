<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="description" content="" />
    <meta name="keywords" content="" />
    <script src="App/boot/require.config.js"></script>
    <script data-main="App/boot/startup" src="Scripts/require/require.js"></script>
    <title>@ViewBag.Title -GameContester</title>

    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/skel.min.js"></script>
    <script src="~/Scripts/skel-layers.min.js"></script>
    <script src="~/Scripts/init.js"></script>

    <link rel="stylesheet" href="~/Content/skel.css" />
    <link rel="stylesheet" href="~/Content/style.css" />
    <link rel="stylesheet" href="~/Content/style-xlarge.css" />

   
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <header id="header">
        <h1><a href="#">Transit</a></h1>
        <nav id="nav">
            <ul>
                <li><a href="#">Home</a></li>
                <li><a href="#register" id="registerLink" class="button special">Sign Up</a></li>
                <li><a href="#login" id="loginLink" class="button special">Sign In</a></li>

            </ul>
        </nav>
    </header>

    <div class="container body-content">
        <div id="page1" class="container" style="padding-top:50px" data-bind="component: { name: route().page, params: route }"></div>
    </div>
    <div class="container body-content">
        @RenderBody()
    </div>
</body>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    

</html>
